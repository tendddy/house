package com.cninsure.payment.model;

/**
 * 1375-市场订单快捷支付(发送验证短信) <br>
 *
 * R 报文中该元素必须出现（Required）<br>
 * O 报文中该元素可选出现（Optional）<br>
 * C 报文中该元素在一定条件下可选出现（Conditional）<br>
 */
public class OrderQuickPay1375Model {

	/**
	 * R
	 * 
	 * 机构号码
	 */
	private String institutionID;
	
	/**
	 * R
	 * 
	 *绑定流水号
	 */
	private String txSNBinding;
	
	/**
	 * R
	 * 
	 * 市场订单号
	 */
	private String orderNo;
	/**
	 * R
	 * 
	 * 支付交易流水号（保证唯一不重复）
	 */
	private String paymentNo;
	/**
	 * R
	 * 
	 * 支付金额，单位：分
	 */
	private String amount;
	/**
	 * O
	 * 
	 * 备注
	 */
	private String remark;
	public String getInstitutionID() {
		return institutionID;
	}
	public void setInstitutionID(String institutionID) {
		this.institutionID = institutionID;
	}
	public String getTxSNBinding() {
		return txSNBinding;
	}
	public void setTxSNBinding(String txSNBinding) {
		this.txSNBinding = txSNBinding;
	}
	public String getOrderNo() {
		return orderNo;
	}
	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}
	public String getPaymentNo() {
		return paymentNo;
	}
	public void setPaymentNo(String paymentNo) {
		this.paymentNo = paymentNo;
	}
	public String getAmount() {
		return amount;
	}
	public void setAmount(String amount) {
		this.amount = amount;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	@Override
	public String toString() {
		return "OrderQuickPay1375Model [institutionID=" + institutionID
				+ ", txSNBinding=" + txSNBinding + ", orderNo=" + orderNo
				+ ", paymentNo=" + paymentNo + ", amount=" + amount
				+ ", remark=" + remark + "]";
	}

	
}
