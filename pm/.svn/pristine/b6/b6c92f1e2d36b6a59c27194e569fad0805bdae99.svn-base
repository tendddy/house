package com.cninsure.payment.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.cninsure.core.dao.impl.BaseDaoImpl;
import com.cninsure.payment.dao.PMApppaymentDao;
import com.cninsure.payment.entity.PMApppayment;
import com.cninsure.payment.entity.PMPaymentplatform;

@Repository
public class PMApppaymentDaoImpl extends BaseDaoImpl<PMApppayment,Long> implements
		PMApppaymentDao {

	@Override
	public List<PMPaymentplatform> quertPaymentsByAppId(long appId) {
		return this.sqlSessionTemplate.selectList(this.getSqlName("quertPaymentsByAppId"), appId);
	}

}