package com.cninsure.payment.model;

import java.io.Serializable;

public class PMPaychannelModel implements Serializable {
	private static final long serialVersionUID = 1L;

	/**
	 * 数据主键
	 */
	private Long id;

	/**
	 * 支付平台ID
	 */
	private Long paymentId;

	/**
	 * 渠道类型 01-银行 02-其它
	 */
	private String channelType;

	/**
	 * 渠道名称，银行名称、第三方平台名称等
	 */
	private String channelName;

	/**
	 * 渠道代码
	 */
	private String channelCode;

	/**
	 * 渠道图片
	 */
	private String channelLogo;

	/**
	 * 客户银行卡号
	 */
	private String customCardNo;
	/**
	 * 客户银行卡号后四位
	 */
	private String shortCardNo;

	/**
	 * 每笔最高额度 －1为不限额度
	 */
	private Integer maxQuotaPerTime;

	/**
	 * 每日最高额度 －1为不限额度
	 */
	private Integer maxQuotaPerDay;

	/**
	 * 绑卡流水号
	 */
	private String protocolId;
	/**
	 * 卡类型
	 */
	private String cardtype;
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getPaymentId() {
		return paymentId;
	}

	public void setPaymentId(Long paymentId) {
		this.paymentId = paymentId;
	}

	public String getChannelType() {
		return channelType;
	}

	public void setChannelType(String channelType) {
		this.channelType = channelType;
	}

	public String getChannelName() {
		return channelName;
	}

	public void setChannelName(String channelName) {
		this.channelName = channelName;
	}

	public String getChannelCode() {
		return channelCode;
	}

	public void setChannelCode(String channelCode) {
		this.channelCode = channelCode;
	}

	public String getChannelLogo() {
		return channelLogo;
	}

	public void setChannelLogo(String channelLogo) {
		this.channelLogo = channelLogo;
	}

	public String getCustomCardNo() {
		return customCardNo;
	}

	public void setCustomCardNo(String customCardNo) {
		this.customCardNo = customCardNo;
	}

	public Integer getMaxQuotaPerTime() {
		return maxQuotaPerTime;
	}

	public void setMaxQuotaPerTime(Integer maxQuotaPerTime) {
		this.maxQuotaPerTime = maxQuotaPerTime;
	}

	public Integer getMaxQuotaPerDay() {
		return maxQuotaPerDay;
	}

	public void setMaxQuotaPerDay(Integer maxQuotaPerDay) {
		this.maxQuotaPerDay = maxQuotaPerDay;
	}
	public String getProtocolId() {
		return protocolId;
	}

	public void setProtocolId(String protocolId) {
		this.protocolId = protocolId;
	}

	public String getCardtype() {
		return cardtype;
	}

	public void setCardtype(String cardtype) {
		this.cardtype = cardtype;
	}

	public String getShortCardNo() {
		return shortCardNo;
	}

	public void setShortCardNo(String shortCardNo) {
		this.shortCardNo = shortCardNo;
	}

	@Override
	public String toString() {
		return "PMPaychannelModel [id=" + id + ", paymentId=" + paymentId
				+ ", channelType=" + channelType + ", channelName="
				+ channelName + ", channelCode=" + channelCode
				+ ", channelLogo=" + channelLogo + ", customCardNo="
				+ customCardNo + ", shortCardNo=" + shortCardNo
				+ ", maxQuotaPerTime=" + maxQuotaPerTime + ", maxQuotaPerDay="
				+ maxQuotaPerDay + ", protocolId=" + protocolId + ", cardtype="
				+ cardtype + "]";
	}

}