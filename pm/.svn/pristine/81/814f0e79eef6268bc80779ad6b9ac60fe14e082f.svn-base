package com.cninsure.payment.Enumm;


public enum BankCodeEnum {
	
	JT("cs0","交通银行"),
	ZS("700","招商银行"),
	GS("cs2","工商银行"),
	ZG("cs3","中国银行"),
	JS("cs4","建设银行");

	private final String key;
	private final String value;
	
	private BankCodeEnum(String key, String value) {
		this.value = value;
		this.key = key;
	}

	public String getKey() {
		return key;
	}

	public String getValue() {
		return value;
	}

	public static String getValueByKey(String key){
		for(BankCodeEnum code:BankCodeEnum.values()){
			if(code.getKey().equals(key)){
				return code.getValue();
			}
		}
		return null;
	}
	
	
	
}
