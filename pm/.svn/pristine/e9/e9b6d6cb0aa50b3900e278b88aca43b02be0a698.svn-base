package com.cninsure.payment.entity;

import java.io.Serializable;

import java.util.Date;

public class PMAPPplatformaccount implements Serializable {
	private static final long serialVersionUID = 1L;

	/**
	 * 主键
	 */
	private Long id;

	/**
	 * 操作员
	 */
	private String operator;

	/**
	 * 创建时间
	 */
	private Date createtime;

	/**
	 * 修改时间
	 */
	private Date modifytime;

	/**
	 * 备注
	 */
	private String noti;

	/**
	 * 应用平台ID
	 */
	private Long app_id;

	/**
	 * 账户总金额
	 */
	private String totalmoney;

	/**
	 * 冻结金额
	 */
	private String freezemoney;

	/**
	 * 可用金额
	 */
	private String usablemoney;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public Date getCreatetime() {
		return createtime;
	}

	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}

	public Date getModifytime() {
		return modifytime;
	}

	public void setModifytime(Date modifytime) {
		this.modifytime = modifytime;
	}

	public String getNoti() {
		return noti;
	}

	public void setNoti(String noti) {
		this.noti = noti;
	}

	public Long getApp_id() {
		return app_id;
	}

	public void setApp_id(Long app_id) {
		this.app_id = app_id;
	}

	public String getTotalmoney() {
		return totalmoney;
	}

	public void setTotalmoney(String totalmoney) {
		this.totalmoney = totalmoney;
	}

	public String getFreezemoney() {
		return freezemoney;
	}

	public void setFreezemoney(String freezemoney) {
		this.freezemoney = freezemoney;
	}

	public String getUsablemoney() {
		return usablemoney;
	}

	public void setUsablemoney(String usablemoney) {
		this.usablemoney = usablemoney;
	}

}