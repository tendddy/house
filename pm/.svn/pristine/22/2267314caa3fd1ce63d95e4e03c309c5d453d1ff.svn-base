package com.cninsure.payment.model;

import java.io.Serializable;

/**
 * 商联绑卡接口参数
 */
public class CustomBindWithCodeModel implements Serializable {
	private static final long serialVersionUID = -7261074772820425726L;

	/**
	 * 支付平台Id
	 */
	private long paymentPlatformId;

	/**
	 * 应用平台Id
	 */
	private long appId;

	/**
	 * 用户Id
	 */
	private String platformUserId;

	/**
	 * 绑卡协议号
	 */
	private String protocolid;

	/**
	 * 验证码
	 */
	private String SMSValidationCode;
	/**
	 * 订单ID
	 */
	private String paymentId;
	public long getPaymentPlatformId() {
		return paymentPlatformId;
	}

	public void setPaymentPlatformId(long paymentPlatformId) {
		this.paymentPlatformId = paymentPlatformId;
	}

	public long getAppId() {
		return appId;
	}

	public void setAppId(long appId) {
		this.appId = appId;
	}

	public String getPlatformUserId() {
		return platformUserId;
	}

	public void setPlatformUserId(String platformUserId) {
		this.platformUserId = platformUserId;
	}

	public String getProtocolid() {
		return protocolid;
	}

	public void setProtocolid(String protocolid) {
		this.protocolid = protocolid;
	}

	public String getSMSValidationCode() {
		return SMSValidationCode;
	}

	public void setSMSValidationCode(String sMSValidationCode) {
		SMSValidationCode = sMSValidationCode;
	}

	public String getPaymentId() {
		return paymentId;
	}

	public void setPaymentId(String paymentId) {
		this.paymentId = paymentId;
	}

	@Override
	public String toString() {
		return "CustomBindWithCodeModel [paymentPlatformId="
				+ paymentPlatformId + ", appId=" + appId + ", platformUserId="
				+ platformUserId + ", protocolid=" + protocolid
				+ ", SMSValidationCode=" + SMSValidationCode + ", paymentId="
				+ paymentId + "]";
	}

	

}
