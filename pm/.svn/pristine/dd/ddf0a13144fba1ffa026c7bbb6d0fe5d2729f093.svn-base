package com.cninsure.payment.model;

import org.springframework.format.annotation.DateTimeFormat;

/**
 *订单查询
 */
public class QueryOrderPayModel extends BaseModel{


	/**
	 * 
	 */
	private static final long serialVersionUID = -1026340603224066694L;

	/**
	 * 接口版本号
	 * 
	 * 固定值：v1.0注意为小写字母
	 * 
	 * 不可空
	 */
	private String version;
	
	/**
	 * 字符集
	 * 
	 * 固定值1,表示UTF-8
	 * 
	 * 可为空
	 */
	private Integer charset;
	
	/**
	 * 合作平台编号
	 * 
	 * 不可空
	 */
	private String systemid;
	
	/**
	 * 商户编号
	 * 
	 * 字符串，由商联通付平台分配
	 * 
	 * 不可空
	 */
	private String corpid;
	
	/**
	 * 付费编号
	 * 
	 * 不可空
	 */
	private String corpuserid;
	
	/**
	 * 商户订单号
	 * 
	 * 字符串，只允许使用字母、数字，每商户提交的订单号，必须在合作平台所有交易中唯一
	 * 
	 * 不可空
	 */
	private String orderid;
	
	/**
	 * 客户绑卡协议号
	 * 
	 * 商联系统中唯一编号，类似银行快捷的协议号，用以对应哪个手机绑了哪张银行卡
	 * 
	 * 不可空
	 */
	private String protocolid;
	
	/**
	 * 商户订单提交时间
	 * 
	 * 数字串，共14位，格式为年月日时分秒，例如：20101208103015
	 * 
	 * 不可空
	 */
	@DateTimeFormat(pattern="yyyyMMddHHmmss")
	private String orderdatetime;
	
	/**
	 * 保留字段
	 * 
	 * 字符串(建议填写请求流水号
	 * 
	 * 可为空
	 */
	private String reserved;
	
	/**
	 * 签名字符串
	 * 
	 * 不可空
	 */
	private String signmsg;

	public String getVersion() {
		return version;
	}

	public void setVersion(String version) {
		this.version = version;
	}

	public Integer getCharset() {
		return charset;
	}

	public void setCharset(Integer charset) {
		this.charset = charset;
	}

	public String getSystemid() {
		return systemid;
	}

	public void setSystemid(String systemid) {
		this.systemid = systemid;
	}

	public String getCorpid() {
		return corpid;
	}

	public void setCorpid(String corpid) {
		this.corpid = corpid;
	}

	public String getCorpuserid() {
		return corpuserid;
	}

	public void setCorpuserid(String corpuserid) {
		this.corpuserid = corpuserid;
	}

	public String getOrderid() {
		return orderid;
	}

	public void setOrderid(String orderid) {
		this.orderid = orderid;
	}

	public String getProtocolid() {
		return protocolid;
	}

	public void setProtocolid(String protocolid) {
		this.protocolid = protocolid;
	}

	public String getOrderdatetime() {
		return orderdatetime;
	}

	public void setOrderdatetime(String orderdatetime) {
		this.orderdatetime = orderdatetime;
	}

	public String getReserved() {
		return reserved;
	}

	public void setReserved(String reserved) {
		this.reserved = reserved;
	}

	public String getSignmsg() {
		return signmsg;
	}

	public void setSignmsg(String signmsg) {
		this.signmsg = signmsg;
	}

	@Override
	public String toString() {
		return "QueryOrderPayModel [version=" + version + ", charset="
				+ charset + ", systemid=" + systemid + ", corpid=" + corpid
				+ ", corpuserid=" + corpuserid + ", orderid=" + orderid
				+ ", protocolid=" + protocolid + ", orderdatetime="
				+ orderdatetime + ", reserved=" + reserved + ", signmsg="
				+ signmsg + "]";
	}
	
}
