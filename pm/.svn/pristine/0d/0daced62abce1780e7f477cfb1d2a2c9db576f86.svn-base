package com.cninsure.payment.model;

import java.util.List;

import com.cninsure.payment.entity.PMBanks;

/**
 * 单个支付平台信息初始化
 */
public class PaymentPlatformModel extends BaseModel {

	private static final long serialVersionUID = 7414997977546193329L;

	/**
	 * 默认选中支付平台
	 */
	private Long paymentPlateformId;

	/**
	 * 支付平台下所有渠道
	 */
	private List<PMBanks> allBanks;
	
	/**
	 * 当前用户已经绑定渠道
	 */
	private List<ShowBankCardModel> bindBanks;

	public Long getPaymentPlateformId() {
		return paymentPlateformId;
	}

	public void setPaymentPlateformId(Long paymentPlateformId) {
		this.paymentPlateformId = paymentPlateformId;
	}



	public List<PMBanks> getAllBanks() {
		return allBanks;
	}

	public void setAllBanks(List<PMBanks> allBanks) {
		this.allBanks = allBanks;
	}

	public List<ShowBankCardModel> getBindBanks() {
		return bindBanks;
	}

	public void setBindBanks(List<ShowBankCardModel> bindBanks) {
		this.bindBanks = bindBanks;
	}

	@Override
	public String toString() {
		return "PaymentPlatformModel [paymentPlateformId=" + paymentPlateformId
				+ ", allBanks=" + allBanks + ", bindBanks=" + bindBanks + "]";
	}

}
