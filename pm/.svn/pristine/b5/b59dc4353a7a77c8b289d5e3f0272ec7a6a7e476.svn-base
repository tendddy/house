package com.cninsure.payment.model;

import java.io.Serializable;

/**
 * 产品信息
 */
public class PMProductsModel implements Serializable {
	
	private static final long serialVersionUID = -2485385284230026220L;

	/**
     * 订单Id
     */
    private String appOrderId;
    /**
     * 订单名称
     */
    private String appOrderName;
    
    /**
     * 应用平台Id
     */
    private Long appId;

    /**
     * 订单费用
     */
    private Integer orderfee;
    
    /**
     * 结算：备注
     */
    private String txRemark;
    
    /**
     * 结算：账户类型：11=个人账户 12=企业账户 20=支付账户
     */
    private String accountType;
    
    /**
     * 结算：银行编号
     */
    private String bankID;
    
    /**
     * 结算：账户名称
     */
    private String accountName;
    
    /**
     * 结算：账户号码
     */
    private String accountNumber;
    
    /**
     * 结算：分支行名称
     */
    private String branchName;
    
    /**
     * 结算：分支行省份
     */
    private String province;
    
    /**
     * 结算：分支行城市
     */
    private String city;
    
	public String getAppOrderId() {
		return appOrderId;
	}

	public void setAppOrderId(String appOrderId) {
		this.appOrderId = appOrderId;
	}

	public String getAppOrderName() {
		return appOrderName;
	}

	public void setAppOrderName(String appOrderName) {
		this.appOrderName = appOrderName;
	}

	public String getOrderid() {
		return getAppId().toString().concat(getAppOrderId().toString());
	}


	public Long getAppId() {
		return appId;
	}

	public void setAppId(Long appId) {
		this.appId = appId;
	}

	public Integer getOrderfee() {
		return orderfee;
	}

	public void setOrderfee(Integer orderfee) {
		this.orderfee = orderfee;
	}
	
	


	public String getAccountType() {
		return accountType;
	}

	public void setAccountType(String accountType) {
		this.accountType = accountType;
	}

	public String getBankID() {
		return bankID;
	}

	public void setBankID(String bankID) {
		this.bankID = bankID;
	}

	public String getAccountName() {
		return accountName;
	}

	public void setAccountName(String accountName) {
		this.accountName = accountName;
	}

	public String getAccountNumber() {
		return accountNumber;
	}

	public void setAccountNumber(String accountNumber) {
		this.accountNumber = accountNumber;
	}

	public String getBranchName() {
		return branchName;
	}

	public void setBranchName(String branchName) {
		this.branchName = branchName;
	}

	public String getProvince() {
		return province;
	}

	public void setProvince(String province) {
		this.province = province;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getTxRemark() {
		return txRemark;
	}

	public void setTxRemark(String txRemark) {
		this.txRemark = txRemark;
	}

	@Override
	public String toString() {
		return "PMProductsModel [appOrderId=" + appOrderId + ", appOrderName="
				+ appOrderName + ", appId=" + appId + ", orderfee=" + orderfee
				+ ", txRemark=" + txRemark + ", accountType=" + accountType
				+ ", bankID=" + bankID + ", accountName=" + accountName
				+ ", accountNumber=" + accountNumber + ", branchName="
				+ branchName + ", province=" + province + ", city=" + city
				+ "]";
	}

	

}