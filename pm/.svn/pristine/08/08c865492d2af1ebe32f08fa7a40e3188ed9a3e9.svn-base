package com.cninsure.payment.model;

import com.cninsure.payment.entity.PMPayment;

/**
 * 支付信息相关接口交互model
 * 
 * @author chengxt1202
 *
 */
public class PMPaymentModel extends BaseModel {

	private static final long serialVersionUID = 1L;

	private Long paymentId; // 支付信息ID
	private PMPayment payment; // 支付信息详情

	public Long getPaymentId() {
		return paymentId;
	}

	public void setPaymentId(Long paymentId) {
		this.paymentId = paymentId;
	}

	public PMPayment getPayment() {
		return payment;
	}

	public void setPayment(PMPayment payment) {
		this.payment = payment;
	}

}
