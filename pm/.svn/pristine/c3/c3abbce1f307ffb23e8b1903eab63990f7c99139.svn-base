		(operator,createtime,modifytime,noti,app_id,app_user_id,app_user_name,app_user_idno) 
		values
		(#{operator},#{createtime},#{modifytime},#{noti},#{app_id},#{app_user_id},#{app_user_name},#{app_user_idno}) 
	</insert>
	<select id="PMAPPcustom_select" parameterType="Map" resultType="com.cninsure.payment.entity.PMAPPcustom">
