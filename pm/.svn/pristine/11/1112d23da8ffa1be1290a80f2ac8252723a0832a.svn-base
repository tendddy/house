package com.cninsure.payment.model;

import java.io.Serializable;

/**
 * 优惠券信息
 */
public class PMOppuModel implements Serializable {

	private static final long serialVersionUID = -2485385284230026220L;

	/**
	 * 优惠券Id
	 */
	private String appOppuId;
	/**
	 * 优惠券名称
	 */
	private String appOppuName;

	/**
	 * 应用平台Id
	 */
	private Long appId;

	/**
	 * 优惠券费用
	 */
	private Integer oppufee;

	/**
	 * 结算：备注
	 */
	private String txRemark;

	/**
	 * 卡券类型（优惠券：01，现金券：02）
	 */
	private String oppuType;

	public String getOppuType() {
		return oppuType;
	}

	public void setOppuType(String oppuType) {
		this.oppuType = oppuType;
	}

	public String getAppOppuId() {
		return appOppuId;
	}

	public void setAppOppuId(String appOppuId) {
		this.appOppuId = appOppuId;
	}

	public String getAppOppuName() {
		return appOppuName;
	}

	public void setAppOppuName(String appOppuName) {
		this.appOppuName = appOppuName;
	}

	public Long getAppId() {
		return appId;
	}

	public void setAppId(Long appId) {
		this.appId = appId;
	}

	public Integer getOppufee() {
		return oppufee;
	}

	public void setOppufee(Integer oppufee) {
		this.oppufee = oppufee;
	}

	public String getTxRemark() {
		return txRemark;
	}

	public void setTxRemark(String txRemark) {
		this.txRemark = txRemark;
	}

}