package com.cninsure.payment.entity;

import java.io.Serializable;

import java.util.Date;

public class PMAPPcustomwithdraw implements Serializable {
	private static final long serialVersionUID = 1L;

	/**
	 * 主键
	 */
	private Long id;

	/**
	 * 操作员
	 */
	private String operator;

	/**
	 * 创建时间
	 */
	private Date createtime;

	/**
	 * 修改时间
	 */
	private Date modifytime;

	/**
	 * 备注
	 */
	private String noti;

	/**
	 * 平台用户表id
	 */
	private Long pmappcustom_id;

	/**
	 * 金额
	 */
	private Long amount;

	/**
	 * 提现申请时间
	 */
	private Date applytime;

	/**
	 *  审核时间
	 */
	private Date verifytime;

	/**
	 * 到账时间
	 */
	private Date receivetime;

	/**
	 * 审核状态（01：待审核，02：审核通过，03：审核不通过）
	 */
	private String status;

	/**
	 * 审核人
	 */
	private String verifyperson;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public Date getCreatetime() {
		return createtime;
	}

	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}

	public Date getModifytime() {
		return modifytime;
	}

	public void setModifytime(Date modifytime) {
		this.modifytime = modifytime;
	}

	public String getNoti() {
		return noti;
	}

	public void setNoti(String noti) {
		this.noti = noti;
	}

	public Long getPmappcustom_id() {
		return pmappcustom_id;
	}

	public void setPmappcustom_id(Long pmappcustom_id) {
		this.pmappcustom_id = pmappcustom_id;
	}

	public Long getAmount() {
		return amount;
	}

	public void setAmount(Long amount) {
		this.amount = amount;
	}

	public Date getApplytime() {
		return applytime;
	}

	public void setApplytime(Date applytime) {
		this.applytime = applytime;
	}

	public Date getVerifytime() {
		return verifytime;
	}

	public void setVerifytime(Date verifytime) {
		this.verifytime = verifytime;
	}

	public Date getReceivetime() {
		return receivetime;
	}

	public void setReceivetime(Date receivetime) {
		this.receivetime = receivetime;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getVerifyperson() {
		return verifyperson;
	}

	public void setVerifyperson(String verifyperson) {
		this.verifyperson = verifyperson;
	}

}