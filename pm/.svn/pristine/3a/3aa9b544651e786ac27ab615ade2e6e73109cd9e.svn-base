package com.cninsure.payment.model;

import java.util.List;

import com.cninsure.payment.entity.PMPaymentplatform;

/**
 * 应用平台支付平台信息初始化
 */
public class PaymentPlatformsModel extends BaseModel {

	/**
	 * 
	 */
	private static final long serialVersionUID = 7414997977546193329L;

	/**
	 * 订单信息id
	 */
	private long paymentId;
	
	/**
	 * 默认选中支付平台
	 */
	private Long defaultPaymentPlatformId;

	/**
	 * 当前应用平台关联所有支付平台
	 */
	private List<PMPaymentplatform> paymentPlatforms;
	
	/**
	 * 当前应用平台关联所有支付平台个数
	 */
	private int length;
	
	/**
	 * 应用平台id
	 */
	private long appId;
	
	/**
	 * 用户id
	 */
	private String appUserId;
	/**
	 * 订单编号
	 */
	private String appOrderid;
	/**
	 * 订单时间
	 */
	private String appOrderTime;
	/**
	 * 应付金额
	 */
	private String orderfee;
	
	public Long getDefaultPaymentPlatformId() {
		return defaultPaymentPlatformId;
	}
	public void setDefaultPaymentPlatformId(Long defaultPaymentPlatformId) {
		this.defaultPaymentPlatformId = defaultPaymentPlatformId;
	}
	public List<PMPaymentplatform> getPaymentPlatforms() {
		return paymentPlatforms;
	}
	public void setPaymentPlatforms(List<PMPaymentplatform> paymentPlatforms) {
		this.paymentPlatforms = paymentPlatforms;
	}
	public long getPaymentId() {
		return paymentId;
	}
	public void setPaymentId(long paymentId) {
		this.paymentId = paymentId;
	}
	public int getLength() {
		return length;
	}
	public void setLength(int length) {
		this.length = length;
	}
	public long getAppId() {
		return appId;
	}
	public void setAppId(long appId) {
		this.appId = appId;
	}
	public String getAppUserId() {
		return appUserId;
	}
	public void setAppUserId(String appUserId) {
		this.appUserId = appUserId;
	}
	public String getAppOrderid() {
		return appOrderid;
	}
	public void setAppOrderid(String appOrderid) {
		this.appOrderid = appOrderid;
	}
	public String getAppOrderTime() {
		return appOrderTime;
	}
	public void setAppOrderTime(String appOrderTime) {
		this.appOrderTime = appOrderTime;
	}
	public String getOrderfee() {
		return orderfee;
	}
	public void setOrderfee(String orderfee) {
		this.orderfee = orderfee;
	}
	@Override
	public String toString() {
		return "PaymentPlatformsModel [paymentId=" + paymentId
				+ ", defaultPaymentPlatformId=" + defaultPaymentPlatformId
				+ ", paymentPlatforms=" + paymentPlatforms + ", length="
				+ length + ", appId=" + appId + ", appUserId=" + appUserId
				+ ", appOrderid=" + appOrderid + ", appOrderTime="
				+ appOrderTime + ", orderfee=" + orderfee + "]";
	}
	


}
