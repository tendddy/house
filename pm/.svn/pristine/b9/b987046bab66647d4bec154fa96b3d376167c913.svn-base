package com.cninsure.payment.model;

import java.io.Serializable;
import java.util.List;

public class OrderDetail4ActionModel implements Serializable {
	private static final long serialVersionUID = 1L;


	/**
	 * 应用平台编号
	 */
	private Long appId;
	
	/**
	 * 应用平台用户标记
	 */
	private String appUserId;
	
	/**
	 * 应用平台用户姓名
	 */
	private String appUserName;
	
	/**
	 * 身份证
	 */
	private String idcard;
	
	
	/**
	 * 应用平台订单号
	 */
	private String appOrderId;

	/**
	 * 支付系统使用订单号
	 * 
	 * 只有get方法 appId+appOrderid
	 */
	private String orderid;

	/**
	 * 商户订单金额,以分为单位
	 */
	private Integer orderfee;

	/**
	 * 订单实际支付金额,以分为单位
	 */
	private Integer payfee;
	
	/**
	 * 支付类型        1 :合并支付(1361)  2：分开支付(2011)
	 */
	private String payType;
	
	private List<PMProductsModel> products;
	
	private List<PMOppuModel> oppus;

	/**
	 * 商户订单提交时间,数字串，共14位，格式为年月日时分秒，例如：20101208103015
	 */
	private String orderdatetime;

	/**
	 * 支付结果回调地址
	 */
	private String receiveurl;

	/**
	 * 用户信息回调地址
	 */
	private String userinfourl;
	
	/**
	 *选择支付的方式 
	 *财付通WAP支付：1
		财付通PC支付：2
		微信APP支付：3
		微信扫码支付：4
		微信公众号支付：5
		微信刷卡支付：6
	 */
	private String payWay;
	
	
	public String getPayWay() {
		return payWay;
	}

	public void setPayWay(String payWay) {
		this.payWay = payWay;
	}

	public String getAppOrderId() {
		return appOrderId;
	}

	public void setAppOrderId(String appOrderId) {
		this.appOrderId = appOrderId;
	}

	public Long getAppId() {
		return appId;
	}

	public void setAppId(Long appId) {
		this.appId = appId;
	}

	public String getAppUserId() {
		return appUserId;
	}

	public void setAppUserId(String appUserId) {
		this.appUserId = appUserId;
	}

	public String getAppUserName() {
		return appUserName;
	}

	public void setAppUserName(String appUserName) {
		this.appUserName = appUserName;
	}

	public String getIdcard() {
		return idcard;
	}

	public void setIdcard(String idcard) {
		this.idcard = idcard;
	}
	
	public String getPayType() {
		return payType;
	}

	public void setPayType(String payType) {
		this.payType = payType;
	}

	public List<PMProductsModel> getProducts() {
		return products;
	}

	public void setProducts(List<PMProductsModel> products) {
		this.products = products;
	}

	public void setOrderid(String orderid) {
		this.orderid = orderid;
	}

	public String getOrderid() {
		return orderid;
	}

	public Integer getOrderfee() {
		return orderfee;
	}

	public void setOrderfee(Integer orderfee) {
		this.orderfee = orderfee;
	}

	public Integer getPayfee() {
		return payfee;
	}

	public void setPayfee(Integer payfee) {
		this.payfee = payfee;
	}

	public String getOrderdatetime() {
		return orderdatetime;
	}

	public void setOrderdatetime(String orderdatetime) {
		this.orderdatetime = orderdatetime;
	}

	public String getReceiveurl() {
		return receiveurl;
	}

	public void setReceiveurl(String receiveurl) {
		this.receiveurl = receiveurl;
	}

	public String getUserinfourl() {
		return userinfourl;
	}

	public void setUserinfourl(String userinfourl) {
		this.userinfourl = userinfourl;
	}
	
	public List<PMOppuModel> getOppus() {
		return oppus;
	}

	public void setOppus(List<PMOppuModel> oppus) {
		this.oppus = oppus;
	}

	@Override
	public String toString() {
		return "OrderDetail4ActionModel [appId=" + appId + ", appUserId="
				+ appUserId + ", appUserName=" + appUserName + ", idcard="
				+ idcard + ", orderid=" + orderid + ", orderfee=" + orderfee
				+ ", payfee=" + payfee + ", payType=" + payType + ", products="
				+ products + ", orderdatetime=" + orderdatetime
				+ ", receiveurl=" + receiveurl + ", userinfourl=" + userinfourl
				+ "]";
	}

}