package com.cninsure.payment.dao.impl;

import java.util.Map;

import org.springframework.stereotype.Repository;

import com.cninsure.core.dao.impl.BaseDaoImpl;
import com.cninsure.payment.dao.PMAPPcustomaccountDao;
import com.cninsure.payment.entity.PMAPPcustomaccount;
import com.cninsure.payment.model.CustomAccountModel;

@Repository
public class PMAPPcustomaccountDaoImpl extends BaseDaoImpl<PMAPPcustomaccount, Long> implements PMAPPcustomaccountDao {

	@Override
	public CustomAccountModel queryCustomAccount(Map<String, Object> param)
	{
		return this.sqlSessionTemplate.selectOne("PMAPPcustomaccount_selectAcccount", param);
	}

	@Override
	public CustomAccountModel getCustomAccountById(Long id)
	{
		return this.sqlSessionTemplate.selectOne("PMAPPcustomaccount_selectCustomAccountById", id);
	}

}