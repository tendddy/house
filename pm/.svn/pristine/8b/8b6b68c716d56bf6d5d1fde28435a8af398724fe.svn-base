package com.cninsure.payment.enumm;

/**
 * 金额类型
 * 
 * @author tongchang
 *
 */
public enum BillInOrOutEnum {
	IN("1", "收入"), OUT("0", "支出");
	BillInOrOutEnum(String value, String name) {
		this.value = value;
		this.name = name;
	}

	private final String value;
	private final String name;

	public String getValue()
	{
		return value;
	}

	public String getName()
	{
		return name;
	}

	public static BillInOrOutEnum getEnum(String value)
	{

		for (BillInOrOutEnum e : values())
		{
			if (e.getValue().equals(value))
				return e;
		}
		throw new IllegalArgumentException("No such Enum ! value : " + value);
	}

}
