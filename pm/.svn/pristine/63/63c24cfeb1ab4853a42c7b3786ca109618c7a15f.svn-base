package com.cninsure.payment.entity;

import java.io.Serializable;
import java.util.Date;

public class PMPaychannel implements Serializable {
	private static final long serialVersionUID = 1L;

	/**
	 * 数据主键
	 */
	private Long id;

	/**
	 * 创建时间
	 */
	private Date createdate;

	/**
	 * 最后修改时间
	 */
	private Date modifydate;

	/**
	 * 支付平台ID
	 */
	private Long paymentId;

	/**
	 * 渠道类型  01-银行  02-其它
	 */
	private String channelType;

	/**
	 * 渠道名称，银行名称、第三方平台名称等
	 */
	private String channelName;

	/**
	 * 渠道代码
	 */
	private String channelCode;

	/**
	 * 渠道图片
	 */
	private String channelLogo;

	/**
	 * 到账时间
	 */
	private Integer arrivalTime;

	/**
	 * 渠道状态 0-启用 1-禁用 2-删除
	 */
	private String status;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Date getCreatedate() {
		return createdate;
	}

	public void setCreatedate(Date createdate) {
		this.createdate = createdate;
	}

	public Date getModifydate() {
		return modifydate;
	}

	public void setModifydate(Date modifydate) {
		this.modifydate = modifydate;
	}

	public Long getPaymentId() {
		return paymentId;
	}

	public void setPaymentId(Long paymentId) {
		this.paymentId = paymentId;
	}

	public String getChannelType() {
		return channelType;
	}

	public void setChannelType(String channelType) {
		this.channelType = channelType;
	}

	public String getChannelName() {
		return channelName;
	}

	public void setChannelName(String channelName) {
		this.channelName = channelName;
	}

	public String getChannelCode() {
		return channelCode;
	}

	public void setChannelCode(String channelCode) {
		this.channelCode = channelCode;
	}

	public String getChannelLogo() {
		return channelLogo;
	}

	public void setChannelLogo(String channelLogo) {
		this.channelLogo = channelLogo;
	}

	public Integer getArrivalTime() {
		return arrivalTime;
	}

	public void setArrivalTime(Integer arrivalTime) {
		this.arrivalTime = arrivalTime;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}
	

	@Override
	public String toString() {
		return "PMPaychannel [id=" + id + ", createdate=" + createdate
				+ ", modifydate=" + modifydate + ", paymentId=" + paymentId
				+ ", channelType=" + channelType + ", channelName="
				+ channelName + ", channelCode=" + channelCode
				+ ", channelLogo=" + channelLogo + ", arrivalTime="
				+ arrivalTime + ", status=" + status + "]";
	}

}