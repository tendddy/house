	<select id="PMAPPcustomaccount_selectAcccount" parameterType="Map" resultMap="customAccountMap">
		SELECT
		c.app_id,c.app_user_id,c.app_user_idno,c.app_user_name,c.login_pwd,c.pay_pwd,ca.id,ca.pmappcustom_id,ca.totalmoney,
		ca.freezemoney,ca.yesterdayownmoney,ca.totalownmoney
		FROM pmappcustom c, pmappcustomaccount ca
		<where>
			c.id = ca.pmappcustom_id
			AND c.app_id = #{appId}
			AND c.app_user_id = #{appUserId}
			AND c.app_user_idno =
			#{appUserIdno}
		</where>
	</select>

	<select id="PMAPPcustomaccount_selectCustomAccountById" parameterType="Long" resultMap="customAccountMap">
