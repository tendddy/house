	<insert id="PMAPPcustomaccount_insert" parameterType="com.cninsure.payment.entity.PMAPPcustomaccount"
		useGeneratedKeys="true" keyProperty="id">
		insert into PMAPPcustomaccount
		(operator,createtime,modifytime,noti,pmappcustom_id,totalmoney,freezemoney,yesterdayownmoney,totalownmoney)
		values
		(#{operator},#{createtime},#{modifytime},#{noti},#{pmappcustom_id},#{totalmoney},#{freezemoney},#{yesterdayownmoney},#{totalownmoney})
	</insert>
