<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cninsure.payment.entity">
	<select id="PMAPPcustomwithdraw_updateByIdSelective" parameterType="com.cninsure.payment.entity.PMAPPcustomwithdraw">
		update PMAPPcustomwithdraw
		<set>
			<if test="applytime != null">
				applytime = #{applytime,jdbcType=TIMESTAMP},
			</if>
			<if test="verifytime !=null">
				verifytime = #{verifytime,jdbcType=TIMESTAMP},
			</if>
			<if test="verifyperson != null">
				verifyperson = #{verifyperson,jdbcType=VARCHAR},
			</if>
			<if test="receivetime !=null">
				receivetime = #{receivetime,jdbcType=TIMESTAMP},
			</if>
			<if test="amount != null">
				amount = #{amount,jdbcType=VARCHAR},
			</if>
			<if test="status != null">
				status = #{status,jdbcType=VARCHAR},
			</if>
		</set>
		where id = #{id,jdbcType=BIGINT}
	</select>

</mapper>