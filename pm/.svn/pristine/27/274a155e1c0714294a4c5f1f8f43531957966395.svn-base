package com.cninsure.payment.entity;

import java.io.Serializable;
import java.util.Date;

public class PMBankcard implements Serializable {
	private static final long serialVersionUID = 1L;

	/**
	 * 数据主键
	 */
	private Long id;

	/**
	 * 创建时间
	 */
	private Date createdate;

	/**
	 * 最后修改时间
	 */
	private Date modifydate;

	/**
	 * 平台ID
	 */
	private Long paymentPlatformId;

	/**
	 * 渠道ID
	 */
	private Long channelId;

	/**
	 * 客户绑卡协议号
	 */
	private String protocolid;

	/**
	 * 客户解绑协议号
	 */
	private String cprotocalid;

	/**
	 * 银行预留手机号
	 */
	private String customphone;

	/**
	 * 银行卡号
	 */
	private String customcardno;

	/**
	 * 用户名
	 */
	private String customname;

	/**
	 * 卡类型 10-个人借记 20-个人贷记
	 */
	private String cardType;

	/**
	 * 开户证件类型： 01 身份证 02 户口簿 03 护照 04 军官证 05 士兵证 06 港澳居民来往内地通行证 07 台湾同胞来往内地通行证
	 * 08 临时身份证 09 外国人居留证 10 警官证 99 其他证件
	 */
	private String identificationType;

	/**
	 * 证件号码
	 */
	private String identificationNumber;

	/**
	 * 信用卡有效期
	 */
	private String validdate;

	/**
	 * 信用卡背面的末3位数字
	 */
	private String cvn2;

	/**
	 * 银行编码
	 */
	private String bankcode;

	/**
	 * 银行名称
	 */
	private String bankname;

	/**
	 * 解除绑定日期
	 */
	private Date removeTime;

	/**
	 * 银行卡状态 0-暂存 1-已绑定 2-解除绑定 3-绑定失败
	 */
	private String status;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Date getCreatedate() {
		return createdate;
	}

	public void setCreatedate(Date createdate) {
		this.createdate = createdate;
	}

	public Date getModifydate() {
		return modifydate;
	}

	public void setModifydate(Date modifydate) {
		this.modifydate = modifydate;
	}

	public Long getPaymentPlatformId() {
		return paymentPlatformId;
	}

	public void setPaymentPlatformId(Long paymentPlatformId) {
		this.paymentPlatformId = paymentPlatformId;
	}

	public Long getChannelId() {
		return channelId;
	}

	public void setChannelId(Long channelId) {
		this.channelId = channelId;
	}

	public String getProtocolid() {
		return protocolid;
	}

	public void setProtocolid(String protocolid) {
		this.protocolid = protocolid;
	}

	public String getCprotocalid() {
		return cprotocalid;
	}

	public void setCprotocalid(String cprotocalid) {
		this.cprotocalid = cprotocalid;
	}

	public String getCustomphone() {
		return customphone;
	}

	public void setCustomphone(String customphone) {
		this.customphone = customphone;
	}

	public String getCustomcardno() {
		return customcardno;
	}

	public void setCustomcardno(String customcardno) {
		this.customcardno = customcardno;
	}

	public String getCustomname() {
		return customname;
	}

	public void setCustomname(String customname) {
		this.customname = customname;
	}

	public String getCardType() {
		return cardType;
	}

	public void setCardType(String cardType) {
		this.cardType = cardType;
	}

	public String getIdentificationType() {
		return identificationType;
	}

	public void setIdentificationType(String identificationType) {
		this.identificationType = identificationType;
	}

	public String getIdentificationNumber() {
		return identificationNumber;
	}

	public void setIdentificationNumber(String identificationNumber) {
		this.identificationNumber = identificationNumber;
	}

	public String getValiddate() {
		return validdate;
	}

	public void setValiddate(String validdate) {
		this.validdate = validdate;
	}

	public String getCvn2() {
		return cvn2;
	}

	public void setCvn2(String cvn2) {
		this.cvn2 = cvn2;
	}

	public String getBankcode() {
		return bankcode;
	}

	public void setBankcode(String bankcode) {
		this.bankcode = bankcode;
	}

	public String getBankname() {
		return bankname;
	}

	public void setBankname(String bankname) {
		this.bankname = bankname;
	}

	public Date getRemoveTime() {
		return removeTime;
	}

	public void setRemoveTime(Date removeTime) {
		this.removeTime = removeTime;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	@Override
	public String toString() {
		return "PMBankcard [id=" + id + ", createdate=" + createdate
				+ ", modifydate=" + modifydate + ", paymentPlatformId="
				+ paymentPlatformId + ", channelId=" + channelId
				+ ", protocolid=" + protocolid + ", cprotocalid=" + cprotocalid
				+ ", customphone=" + customphone + ", customcardno="
				+ customcardno + ", customname=" + customname + ", cardType="
				+ cardType + ", identificationType=" + identificationType
				+ ", identificationNumber=" + identificationNumber
				+ ", validdate=" + validdate + ", cvn2=" + cvn2 + ", bankcode="
				+ bankcode + ", bankname=" + bankname + ", removeTime="
				+ removeTime + ", status=" + status + "]";
	}

}