package com.cninsure.payment.model;

/**
 * 应用平台支付入参
 */
public class PaymentCoustomerModel extends BaseModel{


	/**
	 * 
	 */
	private static final long serialVersionUID = -5944288844142927736L;

	/**
	 * 平台编号
	 */
	private long appPlatformId;

	/**
	 * 订单编号
	 */
	private String orderid;

	/**
	 * 应用平台用户
	 */
	private String appUserId;

	/**
	 * 客户真实姓名
	 */
	private String customname;

	/**
	 * 身份证
	 */
	private String idcard;

	/**
	 * 订单金额
	 */
	private Integer orderfee;

	/**
	 * 支付时间
	 */
	private String orderdatetime;

	/**
	 * 支付回写接口地址
	 */
	private String receiveurl;

	/**
	 * 应用平台用户信息回调接口地址
	 */
	private String userinfourl;

	/**
	 * 应用平台支付账号
	 */
	private String appPayCount;

	public String getOrderid() {
		return orderid;
	}

	public void setOrderid(String orderid) {
		this.orderid = orderid;
	}

	public void setAppUserId(String appUserId) {
		this.appUserId = appUserId;
	}

	public String getAppUserId() {
		return appUserId;
	}

	public String getCustomname() {
		return customname;
	}

	public void setCustomname(String customname) {
		this.customname = customname;
	}

	public String getIdcard() {
		return idcard;
	}

	public void setIdcard(String idcard) {
		this.idcard = idcard;
	}

	public long getAppPlatformId() {
		return appPlatformId;
	}

	public void setAppPlatformId(long appPlatformId) {
		this.appPlatformId = appPlatformId;
	}

	public Integer getOrderfee() {
		return orderfee;
	}

	public void setOrderfee(Integer orderfee) {
		this.orderfee = orderfee;
	}

	public String getOrderdatetime() {
		return orderdatetime;
	}

	public void setOrderdatetime(String orderdatetime) {
		this.orderdatetime = orderdatetime;
	}

	public String getReceiveurl() {
		return receiveurl;
	}

	public void setReceiveurl(String receiveurl) {
		this.receiveurl = receiveurl;
	}

	public String getUserinfourl() {
		return userinfourl;
	}

	public void setUserinfourl(String userinfourl) {
		this.userinfourl = userinfourl;
	}

	public String getAppPayCount() {
		return appPayCount;
	}

	public void setAppPayCount(String appPayCount) {
		this.appPayCount = appPayCount;
	}

	@Override
	public String toString() {
		return "PaymentCoustomerModel [appPlatformId=" + appPlatformId
				+ ", orderid=" + orderid + ", appUserId=" + appUserId
				+ ", customname=" + customname + ", idcard=" + idcard
				+ ", orderfee=" + orderfee + ", orderdatetime=" + orderdatetime
				+ ", receiveurl=" + receiveurl + ", userinfourl=" + userinfourl
				+ ", appPayCount=" + appPayCount + "]";
	}
}
